menuitem "stereo utils";

dialog dumpLeftAndRight(left "Left filepath", leftAd "Left address", right "Right filepath", rightAd "Right address", width "Width", height "Height"){
    GEL_MemorySave(leftAd, 1, (width*height)/4, left, 8, 0);
    GEL_MemorySave(rightAd, 1, (width*height)/4, right, 8, 0);
}